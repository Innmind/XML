    "type": "library",
    "description": "Library to abstract xml manipulation",
    "keywords": ["xml"],
    "homepage": "http://github.com/Innmind/XML",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "baptiste.langlade@hey.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/XML/issues"
    },
    "require": {
        "php": "~8.2",
        "innmind/immutable": "^4.7.1|~5.0",
        "innmind/filesystem": "~7.0"

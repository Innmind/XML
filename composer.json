{
    "name": "innmind/xml",
    "type": "library",
    "description": "Library to abstract xml manipulation",
    "keywords": ["xml"],
    "homepage": "http://github.com/Innmind/XML",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "langlade.baptiste@gmail.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/XML/issues"
    },
    "require": {
        "php": "~7.4|~8.0",
        "innmind/immutable": "~3.0",
        "innmind/stream": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\Xml\\": "src/"
        },
        "files": ["src/bootstrap.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\Xml\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "vimeo/psalm": "~4.1",
        "innmind/coding-standard": "^1.1"
    }
}
